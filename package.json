{
    "name": "@refactorjs/http-proxy",
    "version": "0.0.3",
    "description": "http-proxy alternative",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/refactorjs/http-proxy.git"
    },
    "license": "MIT",
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "unbuild",
        "preapck": "unbuild",
        "release": "standard-version && git push --follow-tags && pnpm publish",
        "mocha": "mocha test/*-test.js",
        "test": "nyc --reporter=text --reporter=lcov npm run mocha"
    },
    "dependencies": {
        "eventemitter3": "^4.0.0",
        "follow-redirects": "^1.15.1"
    },
    "devDependencies": {
        "async": "^3.0.0",
        "concat-stream": "^2.0.0",
        "expect.js": "~0.3.1",
        "mocha": "^10.0.0",
        "nyc": "^15.0.0",
        "semver": "^7.3.7",
        "socket.io": "^2.5.0",
        "socket.io-client": "^2.5.0",
        "sse": "0.0.8",
        "standard-version": "latest",
        "ts-node": "^10.0.0",
        "typescript": "^4.7.4",
        "unbuild": "^0.7.6",
        "ws": "^8.0.0"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "packageManager": "yarn@3.2.0",
    "bugs": {
        "url": "https://github.com/refactorjs/http-proxy/issues"
    },
    "homepage": "https://github.com/refactorjs/http-proxy#readme",
    "keywords": [
        "http-proxy",
        "node-http-proxy",
        "http-proxy",
        "alternative"
    ],
    "author": ""
}